{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "object-curly-spacing": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "linebreak-style": "off",
    "max-len": ["error", { "ignoreComments": true }]
  },
  "extends": ["eslint:recommended", "google"]
}